{
    "name": "pingencom/pingen2-sdk-php",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Official PHP Pingen SDK for API V2",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "ext-json": "*",
        "league/oauth2-client": "^2.6",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Pingen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pingen GmbH",
            "email": "info@pingen.com"
        }
    ],
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nikic/php-parser": "4.15.*|^5.4",
        "phpunit/phpunit": "^10.5|^11.0",
        "mockery/mockery": "^1.4",
        "symplify/easy-coding-standard-prefixed": "9.2.6",
        "rector/rector": "^0.12.16|^2.0",
        "phpstan/phpstan": "^1.4|^2.1"
    }
}
